package oopClassExample;

public class Box {
	
	//Properties
	public double width;	
	public double height;	
	public double length;
	
	//Methods
	public double volume() {		
		return width * height * length;		
	}
	
	public void setTheDimensions(double w, double h, double l) {
		width = w;
		height = h;
		length = l;
	}

}
